generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model Inquiry {
  id        Int      @id @default(autoincrement())
  name      String
  email     String
  phone     String
  message   String   @db.Text
  createdAt DateTime @default(now())
}
